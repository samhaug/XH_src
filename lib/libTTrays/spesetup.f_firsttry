c-------------------------------------
      subroutine spesetup(nknts)
      parameter (MXKNT=31)
      common/speprm/spknt(MXKNT),qq0(MXKNT,MXKNT),qq(3,MXKNT,MXKNT)
      dimension qqwk(3,MXKNT)
      data spknt/-1.,-0.9410254252195227,-0.8791963108848178,
     -  -0.8144233855401029,-0.7466315467775326,-0.6757663817249555,
     -  -0.6018025624454515,-0.5247544495020673,-0.44468916472042075,
     -  -0.36174220690821945,-0.27613531960156856,
     -  -0.18819571074657682,-0.0983748260520716,
     -  -0.0072637215071875705,0.0843991490965299,
     -  0.17573280333749175,0.26573622090672966,0.3533316023994012,
     -  0.43742764710594384,0.5169966576935936,0.5911539867698252,
     -  0.6592255814311909,0.7207909466685891,0.775694625532151,
     -  0.8240270601232281,0.8660823567735689,0.9023038290306851,
     -  0.9332278869278798,0.9594340596530183,0.9815053497318931,1./

      do i=1,MXKNT
        do j=1,MXKNT
          if(i.eq.j) then
            qq0(j,i)=1.
          else
            qq0(j,i)=0.
          endif
        enddo
      enddo
      do i=1,MXKNT
        call rspln(1,MXKNT,spknt(1),qq0(1,i),qq(1,1,i),qqwk(1,1))
      enddo
      nknts=MXKNT
      return
      end
